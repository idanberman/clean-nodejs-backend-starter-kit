{
  "name": "project-runner-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --mode=production",
    "build:debug": "webpack --mode=development",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/",
    "start:dev": "concurrently --kill-others -r -n \"Webpack,Nodemon\" \"yarn run webpack-watch\" \"yarn run server-watch\"",
    "start:debug": "node --inspect dist/",
    "start:prod": "node dist/",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration:run": "jest --config ./test/narrow-integration-test-config/jest-narrow-integration-test.js",
    "test:integration": "docker-compose stop && docker-compose rm -f && docker-compose build && docker-compose run service-narrow-integration-test",
    "webpack-watch": "webpack --watch  --mode=development --colors",
    "server-watch": "nodemon --watch dist/index.js dist/"
  },
  "dependencies": {
    "@decorators/express": "^2.3.0",
    "@tsed/common": "^5.40.1",
    "@tsed/core": "^5.40.1",
    "@types/body-parser": "^1.17.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.8.0",
    "class-validator": "^0.12.0-rc.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "typeorm": "^0.2.20",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.5",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "concurrently": "^5.0.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "roots": [
      "<rootDir>",
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "./test/jest-setup-file.ts"
    ]
  }
}
